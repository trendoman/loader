{
    "name": "seeren/loader",
    "description": "Universal class loader",
    "keywords": ["loader", "psr-0", "psr-4", "class-map"],
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Cyril Ichti",
        "email": "consultant@seeren.fr",
        "homepage" : "http://www.seeren.fr"
      }
    ],
    "minimum-stability": "dev",
    "require": {
      "php": ">=7.0.1"
    },
    "autoload": {
      "psr-4": {
		"Seeren\\Loader\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
		"Seeren\\Loader\\Test\\": "test/"
      }
    },
    "require-dev": {
        "phpunit/phpunit": "6.1.*",
        "satooshi/php-coveralls": "dev-master"
    }
}
