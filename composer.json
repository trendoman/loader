{
	"name" : "seeren/loader",
	"description" : "Universal class loader",
	"keywords" : [
		"loader",
		"psr-0",
		"psr-4",
		"class-map"
	],
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Cyril Ichti",
			"email" : "consultant@seeren.fr",
			"homepage" : "http://www.seeren.fr"
		}
	],
	"minimum-stability" : "dev",
	"require" : {
		"php" : ">=7.2.0"
	},
	"autoload" : {
		"psr-4" : {
			"Seeren\\Loader\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Seeren\\Loader\\Test\\" : "test/"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "7.0.2",
		"satooshi/php-coveralls" : "2.0.0"
	}
}